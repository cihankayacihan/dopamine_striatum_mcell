VIZ_OUTPUT
{
  MODE = ASCII
  FILENAME = "./viz_data/seed_" & seed & "/Scene"
  MOLECULES
  {
    NAME_LIST {ALL_MOLECULES}
    ITERATION_NUMBERS {ALL_DATA @ [[0 TO 100000 STEP 1000]]}
    ITERATION_NUMBERS {ALL_DATA @ [[100000 TO 1000000 STEP 1000]]}
    ITERATION_NUMBERS {ALL_DATA @ [[1000000 TO 5000000 STEP 10000]]}
    ITERATION_NUMBERS {ALL_DATA @ [[5000000 TO 6000000 STEP 1000]]}
    ITERATION_NUMBERS {ALL_DATA @ [[6000000 TO 10000000 STEP 10000]]}
    ITERATION_NUMBERS {ALL_DATA @ [[10000000 TO 50000000 STEP 100000]]}
  }
}

